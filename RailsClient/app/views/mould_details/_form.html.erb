<style type="text/css">
    input[type="submit"] {
        margin-left: 1%;
        height: 50px;
        cursor: pointer;
    }

    input[type="submit"]:hover {
        background-color: #286090;
    }

    .col-sm-4 {
        float: none;
        text-align: left;
        display: inline-flex;
    }

    label {
        width: 150px;
    }

</style>


<%= form_for(@mould_detail) do |f| %>
    <% if @mould_detail.errors.any? %>
        <div id="error_explanation">
          <h2><%= pluralize(@mould_detail.errors.count, "error") %> prohibited this mould_detail from being saved:</h2>

          <ul>
            <% @mould_detail.errors.full_messages.each do |message| %>
                <li><%= message %></li>
            <% end %>
          </ul>
        </div>
    <% end %>

    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :模具号： %>
        <%= f.text_field :mould_id %>
      </div>

      <div class="col-sm-4">
        <%= f.label :模具型号： %>
        <%= f.text_field :mould_type %>
      </div>

      <div class="col-sm-4">
        <%= f.label :模具供应商： %>
        <%= f.text_field :mould_supplier %>
      </div>
    </div>

    <div style="display: inline-flex; width: 100%; margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :状态： %>
        <%= f.text_field :mould_state %>

        <!--
        <%= f.select :mould_state, options_for_select([['未过期', '未过期'], ['即将过期', '即将过期'], ['已过期', '已过期']], params[:mould_state]),
                     {}, {:style => 'width:35%;'} %>
             -->
      </div>

      <div class="col-sm-4">
        <%= f.label :库位号： %>
        <%= f.text_field :position %>
      </div>

      <div class="col-sm-4">
        <%= f.label :端子莱尼号： %>
        <%= f.text_field :terminal_leoni_no %>
      </div>
    </div>

    <div style="display: inline-flex; width: 100%; margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :端子供应商号： %>
        <%= f.text_field :terminal_supplier %>
      </div>

      <div class="col-sm-4">
        <%= f.label :防水塞： %>
        <%= f.text_field :stopwater %>
      </div>

      <div class="col-sm-4">
        <%= f.label :使用范围： %>
        <%= f.text_field :use_range %>
      </div>
    </div>


    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :电线型号： %>
        <%= f.text_field :wire_type %>
      </div>

      <div class="col-sm-4">
        <%= f.label :电线截面qmm： %>
        <%= f.text_field :wire_cross %>
      </div>

      <div class="col-sm-4">
        <%= f.label :原始参数ch： %>
        <%= f.text_field :original_param_ch %>
      </div>
    </div>

    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :原始参数cw： %>
        <%= f.text_field :original_param_cw %>
      </div>

      <div class="col-sm-4">
        <%= f.label :实测参数ch： %>
        <%= f.text_field :actual_param_ch %>
      </div>

      <div class="col-sm-4">
        <%= f.label :实测参数cw： %>
        <%= f.text_field :actual_param_cw %>
      </div>
    </div>


    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :实测参数ich： %>
        <%= f.text_field :actual_param_ich %>
      </div>

      <div class="col-sm-4">
        <%= f.label :实测参数icw： %>
        <%= f.text_field :actual_param_icw %>
      </div>

      <div class="col-sm-4">
        <%= f.label :步骤dch： %>
        <%= f.text_field :step_dch_id %>
      </div>
    </div>

    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :步骤ich： %>
        <%= f.text_field :step_ich_id %>
      </div>

      <div class="col-sm-4">
        <%= f.label :下次批准日期： %>
        <%= f.text_field :next_time %>
      </div>

      <div class="col-sm-4">
        <%= f.label :芯线上刀： %>
        <%= f.text_field :c_up_knife %>
      </div>
    </div>

    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :绝缘上刀： %>
        <%= f.text_field :i_up_knife %>
      </div>

      <div class="col-sm-4">
        <%= f.label :芯线下刀： %>
        <%= f.text_field :c_down_knife %>
      </div>

      <div class="col-sm-4">
        <%= f.label :绝缘下刀： %>
        <%= f.text_field :i_down_knife %>
      </div>
    </div>


    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :上冲头： %>
        <%= f.text_field :upper_punch %>
      </div>

      <div class="col-sm-4">
        <%= f.label :切断刀： %>
        <%= f.text_field :coc %>
      </div>

      <div class="col-sm-4">
        <%= f.label :切断刀座： %>
        <%= f.text_field :coh %>
      </div>
    </div>


    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :送料爪： %>
        <%= f.text_field :feeding_claw %>
      </div>

      <div class="col-sm-4">
        <%= f.label :后凹模： %>
        <%= f.text_field :after_groove %>
      </div>

      <div class="col-sm-4">
        <%= f.label :前凹模： %>
        <%= f.text_field :before_groove %>
      </div>
    </div>


    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :油杯： %>
        <%= f.text_field :oil_cup %>
      </div>

      <div class="col-sm-4">
        <%= f.label :模具买到日期： %>
        <%= f.text_field :buy_time %>
      </div>

      <div class="col-sm-4">
        <%= f.label :放行报告： %>
        <%= f.text_field :release_report %>
      </div>
    </div>


    <div style="display: inline-flex; width: 100%;margin-bottom: 10px;">
      <div class="col-sm-4">
        <%= f.label :固定资产号： %>
        <%= f.text_field :fixed_asset_id %>
      </div>

      <div class="col-sm-4">
        <%= f.label :是否闲置： %>
        <%= f.text_field :is_idle %>
      </div>

      <div class="col-sm-4">
        <%= f.label :闲置日期： %>
        <%= f.text_field :idle_time %>
      </div>
    </div>

    <hr style="border: 1px solid #000;opacity: 0.5;"/>

    <button type="submit" style="margin-left: 10px;margin-top:0;" class="btn btn-primary" name="commit">完成</button>
<% end %>
