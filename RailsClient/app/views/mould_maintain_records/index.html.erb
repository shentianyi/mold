<p id="notice"><%= notice %></p>

<h1>Listing Mould Maintain Records</h1>

<table>
  <thead>
    <tr>
      <th>Mould</th>
      <th>Count</th>
      <th>Plan date</th>
      <th>Real date</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mould_maintain_records.each do |mould_maintain_record| %>
      <tr>
        <td><%= mould_maintain_record.mould_id %></td>
        <td><%= mould_maintain_record.count %></td>
        <td><%= mould_maintain_record.plan_date %></td>
        <td><%= mould_maintain_record.real_date %></td>
        <td><%= link_to 'Show', mould_maintain_record %></td>
        <td><%= link_to 'Edit', edit_mould_maintain_record_path(mould_maintain_record) %></td>
        <td><%= link_to 'Destroy', mould_maintain_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

